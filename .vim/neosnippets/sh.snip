snippet     binbash
abbr        #!/bin/bash
options     head
    #!/bin/bash



snippet     fori
abbr        for loop
options     head
    for ${1:i} in ${2}
    do
		${3}
    done


snippet     functio
abbr        function(){}
options     head
    function ${1:method_name}() {
		${2}
    }


snippet     test
abbr        [] |&
options     head
    [ ${1} ] ${2:||} { 
		${3}
    }


